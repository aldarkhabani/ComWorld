<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/et_first"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".mainpage1"
    tools:layout_editor_absoluteY="89dp">

    <LinearLayout
        android:id="@+id/et_second"
        android:layout_width="match_parent"
        android:layout_height="162dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="161dp"
            android:importantForAccessibility="no"
            app:srcCompat="@drawable/pppp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_third"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D5E1E6"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:fontFamily="serif"
            android:text="@string/enter"
            android:textAlignment="center"
            android:textColor="@color/blue"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_forth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D5E1E6"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="208dp"
            android:layout_height="31dp"
            android:text="@string/type"
            android:textAlignment="gravity"
            android:textColor="@color/design_default_color_secondary_variant"
            android:textSize="14sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:autofillHints=""
            android:ems="10"
            android:hint="@string/sale"
            android:inputType="text" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_fifth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#D5E1E6"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="209dp"
            android:layout_height="32dp"
            android:text="@string/name1"
            android:textAlignment="textStart"
            android:textColor="@color/design_default_color_secondary_variant"
            android:textSize="14sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/use"
            android:importantForAutofill="no"
            tools:ignore="TextFields" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_sixth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#D5E1E6">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="211dp"
            android:layout_height="wrap_content"
            android:text="@string/invoice"
            android:textAlignment="textStart"
            android:textColor="@color/design_default_color_secondary_variant"
            android:textSize="14sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/invoice"
            android:inputType="text" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_third"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#E3D4D6"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView"
            android:layout_width="208dp"
            android:layout_height="wrap_content"
            android:fontFamily="serif"
            android:text="@string/details"
            android:textAlignment="center"
            android:textColor="@color/design_default_color_primary"
            android:textSize="18sp"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_sixth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#D5E1E6"
        android:backgroundTint="#E3D4D6">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_weight="10"
            android:text="@string/mat"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="@string/number"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:text="@string/price1"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_sevenn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#E3D4D6"
        android:orientation="horizontal">

        <EditText
            android:id="@+id/editTextText4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/mat"
            android:inputType="text"
            android:textAlignment="center" />

        <EditText
            android:id="@+id/editTextNumber"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/number"
            android:inputType="number"
            android:textAlignment="center" />

        <EditText
            android:id="@+id/editTextNumber2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/price1"
            android:inputType="number"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/et_sixx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#E3D4D6">

        <TextView
            android:id="@+id/textView66"
            android:layout_width="211dp"
            android:layout_height="match_parent"
            android:text="@string/total1"
            android:textAlignment="center"
            android:textColor="@color/design_default_color_secondary_variant"
            android:textSize="20sp"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/editTextText33"
            android:layout_width="196dp"
            android:layout_height="wrap_content"
            android:autofillHints="@string/total1"
            android:ems="10"
            android:hint="@string/total1"
            android:inputType="text" />

    </LinearLayout>

    <Button
        android:id="@+id/button22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Save"
        android:textColor="@color/ic_lap_background"
        android:textStyle="bold"
        tools:backgroundTint="#C4B0E6"
        tools:layout_editor_absoluteX="411dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#817C8A"
        android:onClick="gogo"
        android:text="@string/report"
        android:textColor="@color/ic_lap_background"
        tools:layout_editor_absoluteX="411dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:backgroundTint="#817C8A"
        android:onClick="gogo"
        android:text="@string/sit"
        android:textColor="@color/ic_lap_background"
        tools:layout_editor_absoluteX="411dp" />


</LinearLayout>